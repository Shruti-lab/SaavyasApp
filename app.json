{
  "expo": {
    "name": "SaavyasApp",
    "slug": "SaavyasApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deskforcea.SaavyasApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/PixelifySans-Bold.ttf",
            "./assets/fonts/PixelifySans-Medium.ttf",
            "./assets/fonts/PixelifySans-Regular.ttf",
            "./assets/fonts/PixelifySans-SemiBold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e06c6b82-47a8-4c95-98c5-cae7e74a989e"
      }
    },
    "scheme":"SaavyasApp"
  }
}
